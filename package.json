{
  "name": "ClanCode",
  "displayName": "clancode",
  "description": "Code with your clan!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ClanCode.CodeGame"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ClanCode.CodeGame",
        "title": "Code with your Clan"
      },
      {
        "command": "ClanCode.refresh",
        "title": "Refresh",
        "icon": {
          "light": "images/refresh.svg",
          "dark": "images/refresh.svg"
        }
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "clanCode",
          "name": "Clan Code"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "clanCode",
        "contents": "Log in and join a clan to [get started](https://www.npmjs.com/)!\n[Log in](command:ClanCode.CodeGame)"
      }
    ]
  },
  "menus": {
    "view/title": [
      {
        "command": "ClanCode.refresh",
        "when": "view == clanCode",
        "group": "navigation"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.45.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "clipboardy": "^2.3.0",
    "express": "^4.17.1",
    "firebase": "^7.14.3",
    "opn": "^6.0.0",
    "vsls": "^1.0.1830"
  }
}
